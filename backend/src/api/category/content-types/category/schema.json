{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "color": {
      "type": "string"
    },
    "icon": {
      "type": "string"
    },
    "kind": {
      "type": "enumeration",
      "enum": [
        "expense",
        "income"
      ],
      "default": "expense",
      "required": true
    },
    "transactions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::transaction.transaction",
      "mappedBy": "category"
    }
  }
}